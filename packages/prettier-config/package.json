{
  "name": "@qlik/prettier-config",
  "version": "0.4.33",
  "description": "Qlik's shared Prettier config",
  "repository": "git@github.com:qlik-oss/dev-tools-js.git",
  "license": "ISC",
  "main": "./prettier.config.js",
  "scripts": {
    "format:check": "cd ../.. && prettier --check 'packages/prettier-config' --ignore-unknown",
    "format:write": "cd ../.. && prettier --write 'packages/prettier-config' --ignore-unknown"
  },
  "dependencies": {
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-sh": "^0.18.0",
    "prettier-plugin-svelte": "^3.4.1"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
