{
  "name": "@qlik/eslint-config",
  "version": "1.4.13",
  "description": "Qlik's ESLint configs",
  "repository": "git@github.com:qlik-oss/dev-tools-js.git",
  "license": "ISC",
  "type": "module",
  "exports": {
    ".": "./src/index.js"
  },
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": [
    "src",
    "!**/__tests__"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "format:check": "cd ../.. && prettier --check 'packages/eslint-config' --ignore-unknown",
    "format:write": "cd ../.. && prettier --write 'packages/eslint-config' --ignore-unknown",
    "lint": "eslint .",
    "test": "vitest run && ./test/verify-configs.sh"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/parser": "^8.52.0",
    "@typescript-eslint/utils": "^8.52.0",
    "@vitest/eslint-plugin": "^1.6.6",
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-svelte": "^3.14.0",
    "eslint-plugin-testing-library": "^7.15.4",
    "globals": "^17.0.0",
    "svelte-eslint-parser": "^1.4.1",
    "typescript-eslint": "^8.52.0"
  },
  "devDependencies": {
    "@qlik/tsconfig": "workspace:^",
    "@types/confusing-browser-globals": "^1.0.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^25.0.5",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "svelte": "^3.37.0 || ^4.0.0 || ^5.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
