{
  "name": "@qlik/tsconfig",
  "version": "0.3.1",
  "description": "Useful compiler options for compiling a TypeScript project",
  "repository": "git@github.com:qlik-oss/dev-tools-js.git",
  "license": "ISC",
  "main": "recommended.json",
  "files": [
    "node.json",
    "react.json",
    "recommended.json",
    "svelte.json",
    "strictest.json",
    "vue.json"
  ],
  "scripts": {
    "format:check": "cd ../.. && prettier --check 'packages/tsconfig' --ignore-unknown",
    "format:write": "cd ../.. && prettier --write 'packages/tsconfig' --ignore-unknown"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
