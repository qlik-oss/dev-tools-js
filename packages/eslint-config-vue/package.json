{
  "name": "@qlik/eslint-config-vue",
  "version": "0.0.7",
  "description": "Qlik's ESLint config for vue projects",
  "repository": "git@github.com:qlik-oss/dev-tools-js.git",
  "license": "ISC",
  "main": "index.js",
  "files": [
    "typescript.js",
    "jest.js",
    "playwright.js",
    "vitest.js"
  ],
  "scripts": {
    "format:check": "prettier --check '**' --ignore-unknown",
    "format:write": "prettier --write '**' --ignore-unknown",
    "lint": "eslint ."
  },
  "prettier": "@qlik/prettier-config",
  "eslintConfig": {
    "extends": "@qlik/eslint-config/node"
  },
  "dependencies": {
    "@qlik/eslint-config": "workspace:*",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-vue": "9.10.0",
    "vue-eslint-parser": "9.1.1"
  },
  "devDependencies": {
    "@qlik/prettier-config": "workspace:*",
    "eslint": "8.38.0",
    "prettier": "2.8.7"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "optionalDependencies": {
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
